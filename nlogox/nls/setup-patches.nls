; patches-own [
;   tmin
;   tmin-12
;   tmin-12ma
;   tmax
;   tmax-12
;   tmax-12ma
;   prec
;   prec-12
;   prec-12ma
;   latitude
;   longitude
;   patch-coordinates
; ]

to setup-patches
  foreach (patch-coordinates) [
    #i -> ask patch (first #i) (last #i) [
      ls:let #i #i
      set tmin [[value] of patch (first #i) (last #i)] ls:of tmin-ls-model
      set tmin-12 [[value-12] of patch (first #i) (last #i)] ls:of tmin-ls-model
      set tmin-12ma [
        [value-12ma] of patch (first #i) (last #i)
      ] ls:of tmin-ls-model

      set tmax [[value] of patch (first #i) (last #i)] ls:of tmax-ls-model
      set tmax-12 [[value-12] of patch (first #i) (last #i)] ls:of tmax-ls-model
      set tmax-12ma [
        [value-12ma] of patch (first #i) (last #i)
      ] ls:of tmax-ls-model

      set prec [[value] of patch (first #i) (last #i)] ls:of prec-ls-model
      set prec-12 [[value-12] of patch (first #i) (last #i)] ls:of prec-ls-model
      set prec-12ma [
        [value-12ma] of patch (first #i) (last #i)
      ] ls:of prec-ls-model

      set latitude mean (
        [[latitude] of patch (first #i) (last #i)] ls:of tmin-ls-model
      )
      set longitude mean (
        [[longitude] of patch (first #i) (last #i)] ls:of tmin-ls-model
      )
    ]
  ]
end
