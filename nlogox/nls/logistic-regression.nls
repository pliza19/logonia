to-report logistic-regression [#tmin #tmax #prec]
  ifelse (
    (is-false? #tmin) or
    (is-false? #tmax) or
    (is-false? #prec)
  ) [
    report false
  ] [
    let #linear-predictor (
      intercept +
      beta-tmin * #tmin +
      beta-tmax * #tmax +
      beta-prec * #prec
    )

    report (1 / (1 + exp(- #linear-predictor)))
  ]
end
